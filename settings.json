{
"go.buildOnSave": true,
  "go.lintOnSave": true,
  "go.vetOnSave": true,
  "go.useCodeSnippetsOnFunctionSuggest": true,
  "go.formatOnSave": true,
  "go.formatTool": "goreturns",
  "go.autocompleteUnimportedPackages": true,
  "[go]": {
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  }
}